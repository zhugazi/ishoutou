body,html{
	width: 100%;
	height: 100%;
	overflow: hidden;
	position: relative;
}

/*头部开始*/
.ui-container {width: 1120px;margin: 0 auto;}

.dcb-banner {position: absolute;z-index: 2;width: 100%;}
.dcb-banner .dcb-top {height: 54px;line-height: 54px;background:#fff;}
.dcb-banner .dcb-top a {color: #454545;}
.dcb-banner .dcb-top ul li {margin-left: 40px;float: left;}
.dcb-banner .ui-container {position: relative;height: 54px;}

.dcb-banner>img{display: block;width: 100%;}
.dcb-banner>a{display: block;position: absolute;width: 132px;height: 109px;font-size: 14px;color: #fff;text-align: center;top: 355px;left: 50%;padding-top: 18px;}
.dcb-banner>a>span{color: #fffd3d;font-size: 15px;}
.dcb_banner_T1{margin-left: -471px;}
.dcb_banner_T1>span{margin-left: 2px;}
.dcb_banner_T2{margin-left: -259px;}
.dcb_banner_T2>span{margin-left: 13px;}
.dcb_banner_T3{margin-left: 114px;}
.dcb_banner_T4{margin-left: 338px;}
/*头部结束*/

